{{!< default}} {{#post}} <article class="gh-article {{post_class}}">

  <header class="gh-header gh-canvas">
    <img class="gh-author-image" src="/assets/images/profile.png" alt="Stephen Puiszis" />
    <h1 class="gh-title">{{title}}</h1>

      <div class="gh-author-meta">
        <div class="gh-author-links">
          <a href="https://www.linkedin.com/in/stephenpuiszis/" target="_blank" rel="noopener">LinkedIn</a>
          <a href="https://github.com/stephen-puiszis" target="_blank" rel="noopener">Github</a>
          <a href="https://twitter.com/scuba__stephen" target="_blank" rel="noopener">Twitter</a>
          <a href="https://www.instagram.com/scuba__stephen/" target="_blank" rel="noopener">Instagram</a>
        </div>
      </div>
  </header>

  <div class="gh-content gh-canvas">
    {{content}}
  </div>

  </article>
{{/post}}
