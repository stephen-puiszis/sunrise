{{!< default}} {{#post}} <article class="gh-article {{post_class}}">

  <header class="gh-header gh-canvas">
    <h1 class="gh-title">Subscribe to Stephen's Newsletter</h1>
  </header>

  <div class="gh-content gh-canvas">

    {{content}}

    <!-- Begin Mailchimp Signup Form -->
    <div id="mc_embed_signup">
      <form action="https://gmail.us7.list-manage.com/subscribe/post?u=1691194b814b0f44547b849b7&amp;id=96a25821f7"
        method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank"
        novalidate>
        <div id="mc_embed_signup_scroll">

          <input type="email" value="" name="EMAIL" class="email gh-input" id="mce-EMAIL" placeholder="email address" required>

          <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;" aria-hidden="true">
            <input type="text" name="b_1691194b814b0f44547b849b7_96a25821f7" tabindex="-1" value="">
          </div>
          <input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="gh-button gh-button-primary">
        </div>
      </form>
    </div>

    <!--End mc_embed_signup-->

  </div>

  </article>
  {{/post}}
